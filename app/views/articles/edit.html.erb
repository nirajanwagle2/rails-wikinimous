<div class="container">
  <h1>Editing article</h1>
  <div class="form-group">

    <%= form_with model: @article, html: {class:"form-group"} do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :content %>
      <%= f.text_area :content %>
      <%= f.submit %>
    <% end %>
  </div>
</div>p
